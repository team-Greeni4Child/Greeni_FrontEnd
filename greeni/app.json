{
  "expo": {
    "name": "Greeni",
    "slug": "greeni",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#FFD1D1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#FFD1D1"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/images/app_icon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
